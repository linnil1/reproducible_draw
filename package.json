{
    "name": "weather-random",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "deploy": "vite build && cat src/lib/server/cron.js >> .cloudflare/worker.js && wrangler deploy",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "format": "prettier --write .",
        "lint": "prettier --check .",
        "test:unit": "vitest",
        "test": "npm run test:unit -- --run"
    },
    "devDependencies": {
        "@cartamd/plugin-anchor": "^2.1.1",
        "@iconify/svelte": "^4.1.0",
        "@stdlib/random-base-mt19937": "^0.2.1",
        "@sveltejs/adapter-auto": "^3.3.1",
        "@sveltejs/adapter-cloudflare-workers": "^2.6.0",
        "@sveltejs/kit": "^2.9.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.1",
        "@tailwindcss/typography": "^0.5.15",
        "@types/luxon": "^3.4.2",
        "autoprefixer": "^10.4.20",
        "carta-md": "^4.6.7",
        "crypto-js": "^4.2.0",
        "luxon": "^3.5.0",
        "pcg-random": "^2.0.1",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.2",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "svelte": "^5.9.0",
        "svelte-check": "^4.1.1",
        "svelte-i18n": "^4.0.1",
        "svelty-picker": "^5.2.10",
        "tailwindcss": "^3.4.16",
        "typescript": "^5.7.2",
        "vite": "^6.0.3",
        "vitest": "^2.1.8",
        "wrangler": "^3.93.0"
    }
}
